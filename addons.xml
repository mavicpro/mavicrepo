<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.f4mTester" version="2.2.4" name="f4mTester" provider-name="Shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.video.F4mProxy" version="2.2.3"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>f4mTester</summary>
        <description></description>
        <platform>all</platform>
    </extension>
</addon>


<addon id="repository.mavic" name="Mavic official Repo" version="1.1.0" provider-name="Mavic">
