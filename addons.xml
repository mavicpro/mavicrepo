<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.f4mTester" version="2.2.4" name="f4mTester" provider-name="Shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.video.F4mProxy" version="2.2.3"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>f4mTester</summary>
        <description></description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="plugin.video.Mavic"
       name="[COLOR yellow]Mavic[/COLOR]"
       version="1.0.2"
       provider-name="Mavic Pro">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Contenidos Mavic</summary>
    <description lang="en">Todo tipo de contenidos multimedia para disfrutarlo.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.mavic" name="Mavic official Repo" version="1.0.2" provider-name="Mavic">
	<extension point="xbmc.addon.repository" name="Kodi Mavic official Repo">
		<info compressed="false">https://raw.githubusercontent.com/mavicpro/mavicrepo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/mavicpro/mavicrepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/mavicpro/mavicrepo/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Mavic Repo para Kodi</summary>
   		<description lang="en">Instala los Addons Mavic para Kodi.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
